#Region -Unterfunktionen------------------------------------------------------------------------------------------------------------

Func __SubFunction($i_A, $i_B, $s_C="+")
	__ConsoleWrite("_Network: __SubFunction($i_A="&$i_A&", $i_B="&$i_B&", $s_C="&$s_C&")"&@CRLF)
	if $s_C="+" then Return $i_A + $i_B
EndFunc

